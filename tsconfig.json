{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "paths": {
      "@/*": [
        "./src/*"
      ],
      "@db/*": [
        "./src/db/*"
      ],
      "@components/*": [
        "./src/components/*"
      ],
      "@layouts/*": [
        "./src/layouts/*"
      ],
      "@svg/*": [
        "./src/svgs/*"
      ],
      "@ui/*": [
        "./src/ui/*"
      ]
    }
  }
}
